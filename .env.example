# Authentication
AUTH_USERNAME=your_username
AUTH_PASSWORD=your_secure_password

# Server Configuration
SERVER_PORT=3000
TTYD_URL=http://localhost:7681

# ttyd Command Configuration
# Examples:
# TTYD_COMMAND=bash                                    # 基本 bash
# TTYD_COMMAND=tmux attach-session -t webterm          # 连接到 tmux 会话
# TTYD_COMMAND=tmux new-session -A -s webterm          # 创建或连接 tmux 会话
# TTYD_COMMAND=tmux attach-session -t webterm:logs     # 连接到特定窗口
TTYD_COMMAND=bash

# Session Configuration
SESSION_SECRET=generate-a-secure-random-key-here
SESSION_NAME=terminal_session

# Security
SECURE_COOKIE=true
HTTP_ONLY=true

# Environment
ENV=production